{
	"name": "parsle",
	"version": "1.1.1",
	"description": "A parser toolkit for JavaScript",
	"main": "src/index.js",
	"scripts": {
		"test": "eslint --ignore-path .gitignore ./src",
		"compile": "echo Compiled `index.js` file: && reqpack index.js ./src | tee ./index.js",
		"crun": "reqpack index.js ./src ./index.js && node runner.js",
		"pb": "reqpack index.js ./src ./index.js && git add . && echo Commit and bump version then run `npm run ab`",
		"ab": "git push && npm publish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FireCubez/parsle.git"
	},
	"keywords": [
		"parser"
	],
	"author": "FireCubez",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/FireCubez/parsle/issues"
	},
	"homepage": "https://github.com/FireCubez/parsle#readme",
	"dependencies": {
		"@firecubez/req": "^1.2.1",
		"line-column": "^1.0.2",
		"mirrorprop": "^1.0.1"
	}
}
